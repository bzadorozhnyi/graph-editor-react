[
    {
        "selector": "node",
        "style": {
            "background-color": "#808080",
            "border-color": "black",
            "border-style": "solid",
            "border-width": "1px",

            "label": "data(label)",
            "text-halign": "center",
            "text-valign": "center",

            "font-family": ["Lato", "Segoe UI", "Lucida Sans Unicode", "Helvetica Neue", "Helvetica", "Arial", "sans-serif"],
            "font-size": "18px",
            "font-weight": "800",

            "height": "50",
            "width": "50"
        }
    },
    {
        "selector": "node[nodeColor]",
        "style": {
            "background-color": "data(nodeColor)"
        }
    },
    {
        "selector": "node[nodeOpacity]",
        "style": {
            "background-opacity": "data(nodeOpacity)"
        }
    },
    {
        "selector": "node[nodeSize]",
        "style": {
            "height": "data(nodeSize)",
            "width": "data(nodeSize)"
        }
    },
    {
        "selector": "node[borderColor]",
        "style": {
            "border-color": "data(borderColor)"
        }
    },
    {
        "selector": "node[borderWidth]",
        "style": {
            "border-width": "data(borderWidth)"
        }
    },
    {
        "selector": "node[labelColor]",
        "style": {
            "color": "data(labelColor)"
        }
    },
    {
        "selector": "node[labelSize]",
        "style": {
            "font-size": "data(labelSize)"
        }
    },
    {
        "selector": "edge",
        "style": {
            "curve-style": "bezier",

            "font-family": ["Lato", "Segoe UI", "Lucida Sans Unicode", "Helvetica Neue", "Helvetica", "Arial", "sans-serif"],
            "font-size": "18px",
            "font-weight": "800",

            "width": "1"
        }
    },
    {
        "selector": "edge[arrow]",
        "style": {
            "target-arrow-shape": "data(arrow)"
        }
    },
    {
        "selector": "edge[edgeColor]",
        "style": {
            "line-color": "data(edgeColor)",
            "target-arrow-color": "data(edgeColor)"
        }
    },
    {
        "selector": "edge[edgeLineStyle]",
        "style": {
            "line-style": "data(edgeLineStyle)"
        }
    },
    {
        "selector": "edge[edgeOpacity]",
        "style": {
            "line-opacity": "data(edgeOpacity)"
        }
    },
    {
        "selector": "edge[edgeWidth]",
        "style": {
            "width": "data(edgeWidth)"
        }
    },
    {
        "selector": "edge[label]",
        "style": {
            "label": "data(label)"
        }
    },
    {
        "selector": "edge[labelColor]",
        "style": {
            "color": "data(labelColor)"
        }
    },
    {
        "selector": "edge[labelMarginX]",
        "style": {
            "text-margin-x": "data(labelMarginX)"
        }
    },
    {
        "selector": "edge[labelMarginY]",
        "style": {
            "text-margin-y": "data(labelMarginY)"
        }
    },
    {
        "selector": "edge[labelSize]",
        "style": {
            "font-size": "data(labelSize)"
        }
    }
]