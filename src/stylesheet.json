[
    {
        "selector": "node",
        "style": {
            "background-color": "#808080",
            "border-color": "black",
            "border-style": "solid",
            "border-width": "1px",

            "label": "data(label)",
            "text-halign": "center",
            "text-valign": "center",

            "font-family": ["Lato", "Segoe UI", "Lucida Sans Unicode", "Helvetica Neue", "Helvetica", "Arial", "sans-serif"],
            "font-size": "18px",
            "font-weight": "800",

            "height": "50",
            "width": "50"
        }
    },
    {
        "selector": "node[nodeColor]",
        "style": {
            "background-color": "data(nodeColor)"
        }
    },
    {
        "selector": "node[borderColor]",
        "style": {
            "border-color": "data(borderColor)"
        }
    },
    {
        "selector": "node[borderWidth]",
        "style": {
            "border-width": "data(borderWidth)"
        }
    },
    {
        "selector": "node[labelColor]",
        "style": {
            "color": "data(labelColor)"
        }
    },
    {
        "selector": "node[labelSize]",
        "style": {
            "font-size": "data(labelSize)"
        }
    },
    {
        "selector": "node[nodeOpacity]",
        "style": {
            "background-opacity": "data(nodeOpacity)"
        }
    },
    {
        "selector": "node[nodeSize]",
        "style": {
            "height": "data(nodeSize)",
            "width": "data(nodeSize)"
        }
    },
    {
        "selector": "edge",
        "style": {
            "curve-style": "bezier",

            "font-family": ["Lato", "Segoe UI", "Lucida Sans Unicode", "Helvetica Neue", "Helvetica", "Arial", "sans-serif"],
            "font-size": "18px",
            "font-weight": "800",

            "width": "1"
        }
    },
    {
        "selector": "edge[label]",
        "style": {
            "label": "data(label)"
        }
    },
    {
        "selector": "edge[arrow]",
        "style": {
            "target-arrow-shape": "data(arrow)"
        }
    },
    {
        "selector": "edge[color]",
        "style": {
            "line-color": "data(color)",
            "target-arrow-color": "data(color)"
        }
    },
    {
        "selector": "edge[labelSize]",
        "style": {
            "font-size": "data(labelSize)"
        }
    },
    {
        "selector": "edge[lineStyle]",
        "style": {
            "line-style": "data(lineStyle)"
        }
    },
    {
        "selector": "edge[marginX]",
        "style": {
            "text-margin-x": "data(marginX)"
        }
    },
    {
        "selector": "edge[marginY]",
        "style": {
            "text-margin-y": "data(marginY)"
        }
    },
    {
        "selector": "edge[opacity]",
        "style": {
            "line-opacity": "data(opacity)"
        }
    },
    {
        "selector": "edge[labelColor]",
        "style": {
            "color": "data(labelColor)"
        }
    },
    {
        "selector": "edge[width]",
        "style": {
            "width": "data(width)"
        }
    }
]